type Query {
    pets: [Pet]
    owner(id: Int): Owner
    getAdoptablePet: [Pet]
}

type Mutation {
    createPet(name: String, age: Int, type: Animal, owner: OwnerInput) : Pet
    addForAdoptPet(name: String, age: Int, type: Animal, availableDate: Date) : Pet
}

type Subscription {
    getNews: NewsUpdate
}
